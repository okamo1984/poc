{
  "name": "solid-gql-project",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:all": "pnpm run -r --if-present dev",
    "precommit": "lint-staged",
    "format": "prettier --write .",
    "lint": "eslint --ext ts,tsx .",
    "generate": "graphql-codegen --config codegen.yml",
    "prestart": "pnpm generate",
    "predev": "pnpm generate",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@graphql-codegen/typescript-urql": "^3.6.4",
    "@graphql-eslint/eslint-plugin": "^3.10.7",
    "@graphql-tools/mock": "^8.7.3",
    "@graphql-tools/schema": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@unocss/reset": "^0.45.8",
    "eslint": "^8.22.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "typescript": "^4.4.3",
    "vite": "^3.0.4",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "@solid-gql/gql-server": "link:packages/gql-server",
    "@solid-gql/solid-start-app": "link:packages/solid-start-app"
  },
  "packageManager": "pnpm@7.9.3"
}
